{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "sunui-social-button": [
        "packages/social-button/src"
      ],
      "sunui-social-button/*": [
        "packages/social-button/src/*"
      ],
      "@storybook/nextjs": [
        "node_modules/@storybook/nextjs"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".storybook/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}