{
    "name": "@sunui-design/react",
    "version": "0.0.1",
    "description": "A modern React UI library with elegant design and smooth animations",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc --build --verbose --force",
        "clean": "rimraf dist tsconfig.tsbuildinfo",
        "dev": "tsc -w",
        "pack": "npm pack",
        "prepublishOnly": "npm run clean && npm run build",
        "publish:same": "npm publish --no-git-tag-version --force"
    },
    "keywords": [
        "nextjs",
        "react",
        "ui"
    ],
    "author": "Sunzhi",
    "license": "Apache-2.0",
    "dependencies": {
        "@sunui-design/core": "^0.0.1",
        "@sunui-design/floating": "^0.0.24",
        "@sunui-design/social": "^0.0.22",
        "@sunui-design/gradient": "^0.0.1",
        "@sunui-design/filter": "^0.0.1"
    },
    "peerDependencies": {
        "next": ">=12.0.0",
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.0",
        "rimraf": "^5.0.0",
        "typescript": "^5.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}